  Size[] s;
}
class Size{
  Shape[] sh;
}
class Shape{
  Colour[] co;
}
class Colour{
  Price[] p;
}
class Price{
  Model[] m;
}
class Model{
  Type[] t;
}
class Type{
  Warranty[] w;
}
class Warranty{
  Replacement [] r;
}
class Replacement {
  Product[] pro;
}
class Product{
  Dress[] d;
